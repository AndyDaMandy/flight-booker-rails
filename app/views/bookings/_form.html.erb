<h2>Booking Form</h2>
<%= form_with model: @booking do |form| %>
    <%= form.hidden_field :flight, value: @flight %>
    <%= form.fields_for :passengers do |subform| %>
    <%= subform.text_field :name %>
    <%= subform.hidden_field :user, value: current_user %>
    <% end %>
    <%= form.submit "Book Flight" %>
<% end %>